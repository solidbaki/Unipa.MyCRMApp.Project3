@{
    ViewData["title"] = "Admin Panel - Delete Manager";
}
<h4 class="display-5 text-center">Delete Manager</h4>
<br>
<p class="text-center">Delete existing manager from the system</p>
<div class="form-group text-center p-auto">
    <label for="inputManagerNameDelete" class="m-3">Manager Name</label>
    <input type="text" class="form-control my-auto" id="inputManagerNameDelete" aria-describedby="managerName" placeholder="Enter manager name"> <br>
    <button class="btn-lg btn-danger">Delete</button>
</div>